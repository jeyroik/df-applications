{
  "name": "jeyroik/df-applications",
  "description": "DF applications",
  "require": {
    "jeyroik/extas-id": "0.*",
    "jeyroik/extas-exceptions": "0.*",
    "jeyroik/extas-foundation": "6.*",
    "jeyroik/extas-base": "2.*",
    "composer/composer": "2.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "jeyroik/extas-snuffbox": "1.*"
  },
  "autoload": {
    "psr-4": {
      "deflou\\": "src/",
      "tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --coverage-clover=coverage.xml --whitelist=src tests"
  },
  "bin": [
    "src/bin/df"
  ]
}